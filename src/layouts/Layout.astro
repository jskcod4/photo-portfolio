            </ul>
          </div>
        </div>
        <!-- /HORIZONTAL BAR -->

        <div class="clearfix"></div>

        <!-- PROFILE RESPONSIVE -->
        <div class="profile_res_info">
          <div class="profile_img">
            <!-- <Image src={Image02} alt="Profile picture mini" /> -->
            <img src={Image02.src} alt="Profile picture mini" />
          </div>
          <div class="info">
            <div class="info_in">
              <h3>José Angel Silva</h3>
              <div class="social_icons">
                <ul>
                  <!-- <li>
                    <a href="#"><i class="xcon-twitter-1"></i></a>
                  </li>
